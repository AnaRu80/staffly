{
	"extends": "expo/tsconfig.base",
	"compilerOptions": {
		"target": "esnext",
		"module": "esnext",
		"jsx": "react-native",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"moduleResolution": "node",
		"allowSyntheticDefaultImports": true,
		"resolveJsonModule": true,
		"noEmit": true,
		"allowJs": true
	}
}
